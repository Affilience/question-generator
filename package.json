{
  "name": "question-generator-monorepo",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "lint": "turbo lint",
    "dev:app": "turbo dev --filter=@qg/app",
    "dev:website": "turbo dev --filter=@qg/website",
    "build:app": "turbo build --filter=@qg/app",
    "build:website": "turbo build --filter=@qg/website"
  },
  "devDependencies": {
    "turbo": "^2.5.3"
  },
  "packageManager": "npm@10.2.0"
}
